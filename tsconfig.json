{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist/",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["es5", "dom"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["dist"]
}
